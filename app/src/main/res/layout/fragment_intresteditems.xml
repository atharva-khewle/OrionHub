<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Intresteditems">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#151515"
        android:orientation="vertical"
        android:padding="7dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cs1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageView42"
                android:layout_width="44dp"
                android:layout_height="36dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/hd_logo"
                tools:src="@drawable/hd_logo"
                tools:srcCompat="@drawable/hd_logo"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="40dp"
            android:text="Interests"
            android:textColor="#FFFFFF"
            android:textColorHighlight="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textColorLink="#FFFFFF"
            android:textSize="24sp"
            android:textStyle="bold" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp"
            android:orientation="vertical"
            android:gravity="top"
            >
            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="320dp"
                android:rowCount="5"
                android:columnCount="3"
                android:padding="0dp"
                android:id="@+id/cards_layout"
                android:orientation="horizontal">





            </GridLayout>



            <Button
                android:id="@+id/intresteditemsbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:height="55dp"
                android:backgroundTint="#FF5700"
                android:text="Continue"
                android:textColor="#FFFFFF"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FDFDFD"
                android:textColorLink="#FAFAFA"
                app:cornerRadius="20dp" />

        </LinearLayout>


    </LinearLayout>


</FrameLayout>